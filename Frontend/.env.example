# Example Environment Variables for Frontend
# Rename this file to .env and fill in your actual values

# AI Provider: 'openai' or 'openrouter'
VITE_AI_PROVIDER=openrouter

# OpenAI Configuration
# Only needed if VITE_AI_PROVIDER=openai
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-4

# OpenRouter Configuration
# Only needed if VITE_AI_PROVIDER=openrouter
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here
VITE_OPENROUTER_MODEL=google/gemini-2.0-flash-001
VITE_SITE_URL=https://your-site-url.com
VITE_SITE_NAME=EMILIA Therapeutic Assistant

# API Base URL for backend
VITE_API_BASE_URL=http://localhost:3000 